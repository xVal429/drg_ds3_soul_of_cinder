"SpriteCard"
{
	$BASETEXTURE "particle/fastSideSprites/fastSideSprite128_V2"

	
	// Set this if you always want "additive over" blending (ONE:INVSRCALPHA)
	$ADDOVERBLEND 1

// RAMPTEXTURE and POWERFUNCTION are mutually exclusive:	
//	$RAMPTEXTURE "particle/fireGradient.vtf"
	$POWERFUNCTION "[ 2 2 2 ]" //"[1.0 2.5 5]"
	
	$OVERBRIGHTFACTOR 2 //1.5
	$EXTRACTGREENALPHA 1
	$ADDSELF 1 //0.1
	$DEPTHBLEND 1
}
